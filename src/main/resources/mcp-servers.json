      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_MCP_PAT}"
      }
    },
    "whatsapp": {
      "command": "/home/delmas/.local/bin/uv",
      "args": [
        "--directory",
        "/home/delmas/PycharmProjects/whatsapp-mcp/whatsapp-mcp-server",
        "run",
        "main.py"
      ]
    },
    "Python-MCP-Server": {
      "command": "/home/delmas/PycharmProjects/Gestion de projet/.venv/bin/mcp",
      "args": [
        "run",
        "/home/delmas/PycharmProjects/Gestion de projet/server.py"
      ]
    }
  }
}
